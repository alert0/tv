<?xml version="1.0" encoding="utf-8"?>
<!-- 仅仅用于在混淆后，listview item在选中时候出现黄色背景 -->
<selector
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_focused="true" android:drawable="@drawable/tv_tab_default" />
    <item android:state_selected="true" android:drawable="@drawable/tv_tab_default" />
    <item android:state_checked="true" android:drawable="@drawable/tv_tab_default" />
    <item android:state_pressed="true" android:drawable="@drawable/tv_tab_default" />
    <item android:drawable="@drawable/tv_tab_default" />
</selector>